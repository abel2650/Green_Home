@page
@model Green_home.Pages.Shared.Ejendomme

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Ejendomme</title>
</head>
<body>
<div>
    <form method="get">
        <input type="text" name="search" placeholder="Search..." />
    </form>
    <table>
        <thead>
        <tr>
            <th>ID</th>
            <th>Pris</th>
            <th>Kvm</th>
            <th>Energimærke</th>
            <th>By</th>
            <!-- Add other properties here -->
        </tr>
        </thead>
        <tbody>
        @foreach (var ejendom in Model.Ejedomme)
        {
            <tr>
                <td>@ejendom.Id</td>
                <td>@ejendom.Pris</td>
                <td>@ejendom.Kvm</td>
                <td>@ejendom.Energimærke</td>
                <td>@ejendom.By_id</td>
                <!-- Add other property values here -->
            </tr>
        }
        </tbody>
    </table>
</div>
</body>
</html>